{
  "name": "product-service",
  "version": "1.0.0",
  "description": "Serverless aws-nodejs-typescript template",
  "main": "serverless.ts",
  "scripts": {
    "start": "sls offline start",
    "deploy": "sls deploy",
    "remove": "sls remove",
    "lint": "eslint 'src/**/*.{js,ts}'",
    "lint:fix": "eslint --fix 'src/**/*.{js,ts}'",
    "format": "prettier --write 'src/**/*.{js,ts}'",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "docker:up": "cd ./src/dynamodb && docker-compose up -d",
    "seed": "ts-node ./src/dynamodb/seeder.ts"
  },
  "engines": {
    "node": ">=14.15.0"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.428.0",
    "@aws-sdk/lib-dynamodb": "^3.428.0",
    "@middy/core": "^3.4.0",
    "@middy/http-cors": "^4.6.4",
    "@middy/http-error-handler": "^4.6.5",
    "@middy/http-json-body-parser": "^3.4.0",
    "@middy/http-urlencode-path-parser": "^4.6.4",
    "@middy/validator": "^4.6.5",
    "aws-sdk": "^2.1473.0",
    "serverless-auto-swagger": "^2.12.0",
    "serverless-dynamodb-local": "^0.2.40",
    "uuid": "^9.0.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@serverless/typescript": "^3.0.0",
    "@types/aws-lambda": "^8.10.71",
    "@types/jest": "^29.5.5",
    "@types/node": "^14.14.25",
    "@typescript-eslint/eslint-plugin": "^6.7.4",
    "@typescript-eslint/parser": "^6.7.4",
    "esbuild": "^0.14.11",
    "eslint": "^8.50.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.7.0",
    "json-schema-to-ts": "^1.5.0",
    "prettier": "^3.0.3",
    "serverless": "^3.0.0",
    "serverless-dotenv-plugin": "^6.0.0",
    "serverless-esbuild": "^1.23.3",
    "serverless-offline": "^13.1.2",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.4.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.1.3"
  },
  "author": "The serverless webpack authors (https://github.com/elastic-coders/serverless-webpack)",
  "license": "MIT"
}
